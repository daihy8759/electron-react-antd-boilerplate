{
    "parser": "babel-eslint",
    "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": ["markdown", "react", "babel"],
    "rules": {
        "jsx-filename-extension": 0,
        "react/prop-types": 0,
        "react/jsx-one-expression-per-line": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js", ".jsx", ".md"]
            }
        ]
    }
}
